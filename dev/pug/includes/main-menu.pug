block link-section
  - var active = 'Documentación';
  
- var menu = { 'Documentación': '/documentacion.html', 'Casos de éxito': '/casos-exito.html', 'Changelog': '/changelog.html', 'Créditos': '/creditos.html', 'Ir a EDteam': 'https://ed.team/' };

ul
  each val, key in menu
    li(itemprop="url")
      a(href=val itemprop="name" title= "Enlace de " + key)= key
